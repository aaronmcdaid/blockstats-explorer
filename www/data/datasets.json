{
  "version": "1.0",
  "block_range": {
    "start": 0,
    "end": 0
  },
  "datasets": [
    {
      "name": "Complete Analysis",
      "file": "complete_analysis.arrow",
      "columns": {
        "height": {"type": "index", "description": "Block height"},
        "timestamp": {"type": "index", "description": "Block timestamp (Unix seconds)"},
        "tx_count": {"type": "metric", "unit": "transactions", "description": "Number of transactions"},
        "fee_avg": {"type": "metric", "unit": "sat/vB", "description": "Average fee rate"},
        "block_size": {"type": "metric", "unit": "bytes", "description": "Block size in bytes"},
        "tx_size_0": {"type": "metric", "unit": "vbytes", "description": "Minimum transaction size"},
        "tx_size_25": {"type": "metric", "unit": "vbytes", "description": "25th percentile transaction size"},
        "tx_size_50": {"type": "metric", "unit": "vbytes", "description": "Median transaction size"},
        "tx_size_75": {"type": "metric", "unit": "vbytes", "description": "75th percentile transaction size"},
        "tx_size_100": {"type": "metric", "unit": "vbytes", "description": "Maximum transaction size"}
      }
    }
  ]
}
