{
  "name": "bitcoin-fee-explorer",
  "version": "0.1.0",
  "description": "Bitcoin block fee explorer with client-side WASM analytics",
  "main": "www/index.html",
  "scripts": {
    "build": "wasm-pack build --target web --out-dir pkg",
    "serve": "python3 -m http.server 8000 --directory www",
    "dev": "npm run build && npm run serve",
    "clean": "rm -rf pkg/"
  },
  "devDependencies": {
    "wasm-pack": "^0.12.1"
  },
  "dependencies": {
    "plotly.js-dist": "^2.26.0"
  }
}